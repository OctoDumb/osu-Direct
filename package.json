{
  "name": "osu!Direct",
  "version": "1.1.0",
  "description": "Super modern replacement for in-game osu!direct",
  "main": "index.js",
  "build": {
    "appId": "com.octodumb.osudirect",
    "extraFiles": [
      "settings"
    ],
    "win": {
      "target": [
        "NSIS"
      ],
      "icon": "icon.ico"
    }
  },
  "scripts": {
    "start": "electron .",
    "win64": "./node_modules/.bin/electron-builder build",
    "win32": "./node_modules/.bin/electron-builder build --ia32"
  },
  "author": "OctopuSS & 4FuN",
  "license": "ISC",
  "devDependencies": {
    "electron": "^3.0.9",
    "electron-builder": "^20.34.0",
    "electron-packager": "^12.2.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "electron-drag": "^1.2.2",
    "ojsama": "^1.0.19",
    "querystring": "^0.2.0",
    "request": "^2.88.0",
    "request-progress": "^3.0.0"
  }
}
