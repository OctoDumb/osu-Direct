{
  "name": "osu-direct",
  "version": "2.0.0",
  "description": "Super modern replacement for in-game osu!direct",
  "main": "index.js",
  "build": {
    "appId": "com.octodumb.osudirect",
    "extraFiles": [
      "settings"
    ],
    "win": {
      "target": [
        "NSIS"
      ],
      "icon": "icon.ico"
    }
  },
  "scripts": {
    "start": "electron .",
    "build": "./node_modules/.bin/electron-builder build",
    "build32": "./node_modules/.bin/electron-builder build --ia32"
  },
  "author": "OctopuSS & 4FuN",
  "license": "ISC",
  "devDependencies": {
    "electron": "^3.0.9",
    "electron-builder": "^20.34.0",
    "electron-packager": "^12.2.0"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "cheerio": "^1.0.0-rc.2",
    "electron-drag": "^1.2.2",
    "jquery": "^3.3.1",
    "ojsama": "^1.0.19",
    "phantom": "^6.2.0",
    "popper.js": "^1.14.7",
    "querystring": "^0.2.0",
    "request": "^2.88.0",
    "request-progress": "^3.0.0",
    "tough-cookie-filestore": "0.0.1"
  }
}
